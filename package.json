{
  "name": "sql-sheets",
  "displayName": "sql-sheets",
  "description": "Assist with putting data into google sheets at scale.",
  "version": "0.0.1",
  "publisher": "davidsa",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/sql-sheets"
  },
  "icon": "media/icon.png",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:sql"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "sql-parameters-view-container",
          "title": "SQL Parameters",
          "icon": "media/icon.png"
        }
      ]
    },
    "views": {
      "sql-parameters-view-container": [
        {
          "id": "sql-sheets.editor",
          "name": "SQL Sheet Editor",
          "type": "webview"
        }
      ]
    },
    "commands": [
      {
        "command": "sql-sheets.showEditor",
        "title": "sql-sheets: Show SQL Sheet Editor",
        "icon": "$(database)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "package": "npx @vscode/vsce package",
    "install-extension": "/Applications/Visual\\ Studio\\ Code.app/Contents/Resources/app/bin/code --install-extension \"$(ls -t sql-sheets-*.vsix | head -n 1)\""
  },
  "devDependencies": {
    "@types/vscode": "^1.105.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "eslint": "^9.36.0",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^2.24.0"
  },
  "dependencies": {
    "@vscode/webview-ui-toolkit": "^1.2.2"
  }
}