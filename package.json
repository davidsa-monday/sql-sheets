{
  "name": "sql-sheets",
  "displayName": "sql-sheets",
  "description": "Assist with putting data into google sheets at scale.",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/sql-sheets"
  },
  "icon": "media/icon.png",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "sql-parameters-view-container",
          "title": "SQL Parameters",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "sql-parameters-view-container": [
        {
          "id": "sql-sheets.editor",
          "name": "SQL Sheet Editor",
          "type": "webview"
        }
      ]
    },
    "commands": [
      {
        "command": "sql-sheets.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.105.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "eslint": "^9.36.0",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  },
  "dependencies": {
    "@vscode/webview-ui-toolkit": "^1.2.2"
  }
}