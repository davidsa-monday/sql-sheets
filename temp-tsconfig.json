{
    "compilerOptions": {
        "module": "commonjs",
        "target": "ES2022",
        "outDir": "out-temp",
        "lib": [
            "ES2022"
        ],
        "sourceMap": true,
        "rootDir": "src",
        "strict": true,
        "moduleResolution": "node",
        "skipLibCheck": true
    },
    "include": [
        "src"
    ],
    "exclude": [
        "node_modules",
        ".vscode-test"
    ]
}